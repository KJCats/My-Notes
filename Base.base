views:
  - type: table
    name: Table
    order:
      - file.name
      - file.size
      - file.backlinks
    sort:
      - property: file.mtime
        direction: DESC
      - property: file.links
        direction: ASC
  - type: cards
    name: View
    sort:
      - property: file.links
        direction: ASC
    image: file.file
    imageFit: ""
    cardSize: 100
